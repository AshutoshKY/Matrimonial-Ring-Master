            border-top-right-radius:8% 58%;
            background-color:#296868;
            padding-left:5%;
            padding-top:2.5%;
            color:white;
            letter-spacing:2%;
        }
        #chatSpace{
            position:absolute;
            top:13%;
            height:74%;
            width:100%;
            border-bottom:1px solid grey;
            overflow-y: auto;
        }
        #chatMsg{
            position: absolute;
            color:white;
            top:87%;
            height:10%;
            width:100%;
        }
        #msgip{
            position:absolute;
            background-color:#296868;
            padding-left:2%;
